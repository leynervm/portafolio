---
import Experience from "./Experience.astro";
import IconExperience from "./IconExperience.astro";
---

<div class="px-3 md:px-0 w-full">
  <div class="relative pt-5 md:pt-20 content-experiences">
    <h1 class="text-xl md:text-4xl">Experiencia laboral</h1>

    <div class="gap-6 grid lg:grid-cols-2 py-10 md:py-24">
      <Experience
        class="gradiente-bottom-right"
        title="Next Technologies"
        description=`Desarrolador web full stack para la elaboración de 
        sistemas web para la gestion de las actividades de la empresa 
        (Sistemas de soporte técnico y sistema ERP).`
        img="logo-next.png"
      />

      <!-- <Experience
        class="gradiente-top-left"
        title="CIB on the Mobile"
        description=`Take your client onboard seamlessly by our amazing tool of
                digital onboard process.`
        img="bombilla.png"
      /> -->

      <!-- <Experience
